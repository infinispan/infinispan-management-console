{
  "attributes": {
    "cache-container": {
      "select-option": "single",
      "resolve": {
        "service": "containerService",
        "method": "getAllContainers",
        "objectPath": "name"
      }
    },
    "socket-binding": {
      "select-option": "single",
      "resolve": {
        "service": "socketBindingService",
        "method": "getDefaultSocketBindingGroup",
        "objectPath": "name"
      }
    }
  }
}
